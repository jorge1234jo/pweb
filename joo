<script>
var p1f1 = document.getElementById('p1f1');
var p1f2 = document.getElementById('p1f2');
p1f1.addEventListener('mouseover', function() {
p1f1.style.opacity = '0';
p1f2.style.display = 'block';
});
p1f2.addEventListener('mouseout', function() {
p1f1.style.opacity = '1';
p1f2.style.display = 'none';
});
