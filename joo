<script>
var slideIndex = 1; // Comienza en la primera diapositiva
showSlides(slideIndex);

function plusSlides(n) {
  showSlides(slideIndex += n);
}

function showSlides(n) {
  var i;
  var slides = document.getElementsByClassName("mySlides");
  if (n > slides.length) {slideIndex = 1} // Vuelve a la primera diapositiva
  if (n < 1) {slideIndex = slides.length} // Va a la última diapositiva
  for (i = 0; i < slides.length; i++) {
      slides[i].style.display = "none"; // Oculta todas las diapositivas
  }
  slides[slideIndex-1].style.display = "block"; // Muestra la diapositiva actual
}

// Agrega eventos de clic a los botones
document.querySelector('.carousel-button_previous').addEventListener('click', function() {
  plusSlides(-1);
});

document.querySelector('.carousel-button_next').addEventListener('click', function() {
  plusSlides(1);
});
</script>
